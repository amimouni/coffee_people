<%= form_for(@cv) do |f| %>
  <%= f.label :job %>
  <%= f.text_field :job %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <%= f.label :experience %>
  <%= f.text_field :experience %>
  <%= f.label :start_date %>
  <%= f.text_field :start_date %>
  <%= f.label :comment %>
  <%= f.text_field :comment %>
  <%= f.submit %>
<% end %>
